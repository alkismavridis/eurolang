//This was the first eurolang "useful" program to ever compile and run successfully! 2018.09.15

const numToTest = 6

if numToTest % 2 == 0 {
    print("not a prime")
    return 1
}


var d = 3
while (d*d <= numToTest) {
    if numToTest % d == 0 {
        print("not a prime")
        return 2
    }

    d = d+2
}

print("prime")